#include <iostream>
using namespace std;

int main() {
    int num, sum = 0;

    cout << "Enter numbers (enter 0 to stop): " << endl;
    cin >> num;

    while (num != 0) {
        if (num > 0) { 
            sum += num;
        }
        cin >> num; 
    }

    cout << "Sum of positive integers = " << sum << endl;

    return 0;
}
