#include <iostream>
using namespace std;

int main() {
    // Inputs
    int inputs[] = {5, -3, 10, 7, 0};
    int sum = 0;

    cout << "Inputs: ";
    for (int i = 0; inputs[i] != 0; i++) {
        cout << inputs[i] << " ";
        if (inputs[i] > 0) {
            sum += inputs[i];
        }
    }
    cout << "0" << endl; 

    cout << "Sum of positive integers: " << sum << endl;

    return 0;
}
