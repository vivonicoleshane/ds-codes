#include <iostream>
using namespace std;

int main() {
    int grade;

    // Input grade
    grade = 87;
    cout << "Enter grade: " << grade << endl;

    // Using a compound statement with braces
    if (grade >= 75) {
        cout << "Result: Pass" << endl;
        cout << "Congratulations!" << endl;
        cout << "Keep up the good work." << endl;
    } else {
        cout << "Result: Fail" << endl;
    }

    return 0;
}
